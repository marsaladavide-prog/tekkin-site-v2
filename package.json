{
  "name": "tekkin-site-v2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
"scripts": {
  "dev": "concurrently \"npm run dev:next\" \"npm run dev:analyzer\"",
  "build": "next build",
  "start": "next start",
  "lint": "eslint .",
  "lint:fix": "eslint . --fix",
